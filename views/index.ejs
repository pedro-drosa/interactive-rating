<%- include('./partials/head.ejs')%>
  <section class="card">
    <div class="card-icon">
      <img src="assets/icon-star.svg" alt="orange star" />
    </div>
    <h1>How did we do?</h1>
    <p>
      Please let us know how we did with your support request. All feedback is
      appreciated to help us improve our offering!
    </p>
    <form class="form" action="/result">
      <fieldset class="form-container">
        <legend>Ratings for evaluation</legend>
        <label for="one" class="form-button">
          <span>1</span>
          <input id="one" type="radio" value="1" name="evaluationGrade" accesskey="1" />
        </label>
        <label for="two" class="form-button">
          <span>2</span>
          <input id="two" type="radio" value="2" name="evaluationGrade" accesskey="2" />
        </label>
        <label for="three" class="form-button">
          <span>3</span>
          <input id="three" type="radio" value="3" name="evaluationGrade" accesskey="3" />
        </label>
        <label for="four" class="form-button">
          <span>4</span>
          <input id="four" type="radio" value="4" name="evaluationGrade" accesskey="4" />
        </label>
        <label for="five" class="form-button">
          <span>5</span>
          <input id="five" type="radio" value="5" name="evaluationGrade" accesskey="5" />
        </label>
      </fieldset>
      <button type="submit" class="card-button">submit</button>
    </form>
  </section>
<%- include('./partials/body.ejs')%>
